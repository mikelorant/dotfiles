# freshshell.com
#
# Examples:
#
#   fresh twe4ked/dotfiles shell/functions/\*
#   fresh jasoncodes/dotfiles shell/aliases/rails.sh
#   fresh jasoncodes/dotfiles config/tmux.conf --file
#
# See http://freshshell.com/readme for documentation.

fresh freshshell/fresh bin/fresh --bin              # fresh
fresh zsh/* --file                                  # zsh
fresh git/* --file=~/.gitconfig --filter='erb -T 2' # git
fresh nano/* --file                                 # nano
fresh ruby/* --file                                 # ruby
fresh ssh/* --file=~/.ssh/config                    # ssh
fresh awscli/* --file=~/.aws/config                 # awscli
fresh saml2aws/* --file                             # saml2aws
fresh osx/* --file=~/.osx                           # osx
fresh powerlevel10k/* --file ~/.p10k.zsh            # powerlevel10
